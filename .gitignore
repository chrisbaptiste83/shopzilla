# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Ignore the database file.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore credentials
/config/credentials.yml.enc
/config/master.key

# Ignore node_modules
/node_modules

# Ignore yarn-error.log
/yarn-error.log

# Ignore .byebug_history
.byebug_history

# Ignore assets
/app/assets/builds/*
!/app/assets/builds/.keep
.DS_Store\n# Ignore deployment configuration and secrets\nconfig/deploy.yml\n.kamal/secrets
